{
  "name": "kodflix",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001",
  "dependencies": {
    "express": "^4.16.4",
    "nodemon": "^1.18.9",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "start": "npm-run-all -s build _start-backend",
    "start-dev": "npm-run-all -p _start-frontend _start-backend",
